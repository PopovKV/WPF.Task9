<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="WindowStyle">
        <Setter Property="Control.Background" Value="White"/>

    </Style>
    <Style x:Key="TextBoxStyle" BasedOn="{StaticResource WindowStyle}"/>
    <Style x:Key="MenuItemStyle">
        <Setter Property="Control.Foreground" Value="#1a1a1a"/>
        <Setter Property="Control.Background" Value="#f0f0f0"/>
    </Style>
    <Style x:Key="MenuStyle" BasedOn="{StaticResource MenuItemStyle}"/>
    <Style x:Key="ToolBarStyle">
        <Setter Property="Control.Background" Value="#FFEEF5FD"/>
    </Style>
    <Style x:Key="ToolBarTrayStyle" BasedOn="{StaticResource ToolBarStyle}"/>
    <Style x:Key="ComboBoxStyle" BasedOn="{StaticResource WindowStyle}">
        <Setter Property="Control.Background" Value="White"/>
    </Style>
</ResourceDictionary>